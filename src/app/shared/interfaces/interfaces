export interface User
{
  login: string;
  password: string;
  returnSecureToken: boolean;
}

export interface Post{
  id?: number;
  text: string;
  author: string;
  title: string;
  date: Date;
}

export interface FbAuthResponse {
  idToken: string
  expiresIn: number;
}

export interface FbCreateResponse{
  name: string
}
